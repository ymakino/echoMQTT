<rules>
  <publish>
    <address>150.65.230.104</address>
    <eoj>001101</eoj>
    <property>
      <epc>E0</epc>
      <name>temperature</name>
      <converter>
        <class>Float</class>
        <param><name>size</name><value>2</value></param>
        <param><name>divide</name><value>10</value></param>
      </converter>
    </property>
    <interval>10000</interval>
    <topic>devices/device1/event</topic>
  </publish>
  <publish>
    <address>150.65.230.104</address>
    <eoj>001201</eoj>
    <property>
      <epc>E0</epc>
      <name>humidity</name>
      <converter>
        <class>Integer</class>
        <param><name>unsigned</name><value>true</value></param>
        <param><name>size</name><value>1</value></param>
      </converter>
    </property>
    <interval>10000</interval>
    <topic>devices/device2/event</topic>
  </publish>
  <publish>
    <address>150.65.231.30</address>
    <eoj>002d01</eoj>
    <property>
      <epc>E0</epc>
      <name>airpressure</name>
      <converter>
        <class>Float</class>
        <param><name>size</name><value>2</value></param>
        <param><name>divide</name><value>10</value></param>
      </converter>
    </property>
    <interval>10000</interval>
    <topic>devices/device5/event</topic>
  </publish>
  <publish>
    <address>150.65.231.35</address>
    <eoj>001b01</eoj>
    <property>
      <epc>E0</epc>
      <name>co2</name>
      <converter>
        <class>Integer</class>
        <param><name>unsigned</name><value>true</value></param>
        <param><name>size</name><value>2</value></param>
      </converter>
    </property>
    <property>
      <epc>E0</epc>
      <name>eco2</name>
      <converter>
        <class>Integer</class>
        <param><name>unsigned</name><value>true</value></param>
        <param><name>size</name><value>2</value></param>
      </converter>
    </property>
    <interval>10000</interval>
    <topic>devices/device8/event</topic>
  </publish>
  <publish>
    <address>150.65.231.35</address>
    <eoj>001d01</eoj>
    <property>
      <epc>E0</epc>
      <name>tvoc</name>
      <converter>
        <class>Integer</class>
        <param><name>unsigned</name><value>true</value></param>
        <param><name>size</name><value>2</value></param>
      </converter>
    </property>
    <interval>10000</interval>
    <topic>devices/device9/event</topic>
  </publish>

  <subscribe>
    <address>150.65.231.21</address>
    <eoj>05fd01</eoj>
    <property>
      <epc>80</epc>
      <name>lighting</name>
      <converter>
        <class>Map</class>
        <param><name>mapping</name><value>{"ON":"30","OFF":"31"}</value></param>
      </converter>
    </property>
    <topic>devices/device10/event</topic>
  </subscribe>
</rules>